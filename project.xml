<?xml version="1.0" encoding="utf-8"?>
<project>
    <meta title="openfl-svg-aswing-tests" package="openfl-svg-aswing-tests" version="0.0.1" company="Nick Grebenshikov" />

    <!-- output -->
    <app main="Main" file="openfl-svg-aswing-tests" path="bin" />

    <window width="0" height="0" background="0xFFFFFF" fps="60" if="html5 || flash"/>
    <window width="800" height="600" background="0xFFFFFF" fps="125" unless="html5 || flash" />

    <!-- classpath, haxe libs -->
    <source path="src" />

    <set name="html5-backend" value="openfl-snapsvg"/>

    <haxelib name="openfl" />
    <haxelib name="aswing"/>

    <template path="templates" />

    <assets path="assets" rename="" include="*.jpg"/>

</project>